    "express": "^4.18.2",
    "mongoose": "^7.6.0",
    "mongoose-paginate-v2": "^1.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/mongoose": "^5.11.97",
    "@types/mongoose-paginate-v2": "^1.7.0",
    "@types/node": "^20.5.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.0"
